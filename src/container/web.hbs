<div>
  {{#with web}}
    <div><ul>
        {{#each languages}}
          <li>
            <b>{{name}}</b>-
            {{version}}
            {{description}}
          </li>
        
      </ul></div>
    <div>
      <ul>
        {{#each elements}}
          <li><b>{{name}}</b>-
            {{description}}<br>
                {{#each attributes}}
                <li><b>{{name}}</b>-
                {{description}}</li>
                {{/each}}
          </li>
        {{/each}}</ul>
        {{#each properties}}
        <ul>
            <li>
                <b>{{name}}</b>
                {{description}}
                {{#each values}}
              <li>  <b>{{value}}</b>-
                {{description}}</li>
                {{/each}}
            </li>
        </ul>
        {{/each}}
        {{#each functions}}
          <li>  <b>{{name}}</b>-
            {{description}}</li>
            
                {{#each parameters}}
                    <ul>
                        <b>{{name}}</b>-
                        {{type}}<br>
                        {{description}}
                    </ul>
                {{/each}}
            
        {{/each}}
    </div>
    {{/each}}
  {{/with}}
</div>