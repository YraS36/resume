<main>
  <div>
    <div> {{#with navigation}}
      {{#each links}}
        <tr>
          <th>
            <button
              type='button'
              class='btn btn-link'
              href='{{href}}'
            >{{text}}</button>
          </th>
        </tr>
      {{/each}}
      <button
        type='button'
        class='btn btn-primary'
        href='{{button.href}}'
      >
        {{button.text}}
      </button>
      <hr />
    {{/with}}
    </div>
    <div>{{#with header}}
      <h1>{{title}}</h1>
      <p>{{description}}</p>
      <button
        type='button'
        class='btn btn-primary'
        href='{{button.link}}'
      >{{button.text}}</button>
    {{/with}}
    </div>
    <hr />

    {{#with city}}
      <h2>{{title}}</h2>
      <br>
    <section class="row  g-4">
        {{#each cards}}
        <div class="col-6">
            <div class='card'>  
                 {{#with picture}}
                    <img
                    class='card-img-top'
                    src='{{src}}'
                    alt='{{alt}}'
                    />
                   {{/with}}
                <div class='card-body'>
                    <h5 class='card-title'>{{title}}</h5>
                    <p>{{description}} </p>
                    {{#with button}}
                    <button
                        type='button'
                        class='btn btn-primary'
                        href='{{href}}'
                    >{{text}}</button>
                    {{/with}}
                </div>
            </div>
        </div> 
        {{/each}}
    </section>
    {{/with}}
    <hr />
    <br>
    {{#with winner}}
      <h2>{{title}}</h2>
      {{#with table}}
        <table class='table'>
          <thead>
            <tr>
                {{#each head}}
                    <th>{{this}}</th>
                    {{/each}}
            </tr>
          </thead>
          <tbody>
            <tr>{{#each user}}
              <th>{{place}}</th>
              <td>{{name}}</td>
              <td>{{surname}}</td>
              <td>{{nickname}}</td>
            </tr>{{/each}}
          </tbody>
        </table>
      {{/with}}
    {{/with}}
    <hr>
    <br>
    {{#with gallery}}  
    <h2>{{title}}</h2>
    <section class="row">
    {{#each cards}}
    
    <div class="col-4">
    <div class="card">
        <div class="card-header">
                {{name}}</div>

        <ul class="list-group list-group-flush">
                {{#each items}}
                <li class="list-group-item">{{this}}</li>
                {{/each}}
            
        </ul>
        </div>
    </div>
    {{/each}}
    </section>
    {{/with}}
    <br>
    <hr>
    {{#with relaxion}}
        <p class="display-3">{{title}}</p>
        <section class="row">
        {{#each cards}}
        <div class="col-6">
        <div class="card text-bg-secondary">
            <div class="card-header">{{header}}</div>
            <div class="card-body">
            <div class="card-title">{{title}}</div>
            <p>{{description}}</p>
        </div>
        </div>
        </div> 
        {{/each}}
        </section>
        {{/with}}
        <hr>
    {{#with rest}}
        <p class="display-3">{{title}}</p>
        <section class="row">
        {{#each cards}}
        <div class="col-6">
        <div class="card">
            <div class="card-header text-center">{{header}}</div>
            <div class="card-body text-center">
            <div class="card-title">{{title}}</div>
            <p class="">{{description}}</p>
            <button type="button" class="btn btn-primary text-center">{{button}}</button>
            <br>
            <br>
            <p class="card-footer">{{footer}} </p>
        </div>
        </div>
        </div> 
        {{/each}}
        </section>
    {{/with}}
    <hr>
    {{#with proposition}}
        <p class="display-3">{{title}}</p>
        <section class="row">
        {{#each card}}
        <div class="col-6">
        <div class="card text-bg-warning">
            <div class="card-header">{{header}}</div>
            <div class="card-body">
            <div class="card-title">{{title}}</div>
            <p class="">{{description}}</p> 
        </div>
        </div>
        </div>
        {{/each}}
        </section>
    {{/with}}

    


  </div>
</main>